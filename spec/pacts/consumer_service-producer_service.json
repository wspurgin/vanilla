{
  "consumer": {
    "name": "Consumer Service"
  },
  "provider": {
    "name": "Producer Service"
  },
  "interactions": [
    {
      "description": "a request to get data for multiple Things",
      "providerState": "things exist",
      "request": {
        "method": "get",
        "path": "/things",
        "query": "ids=12345&ids=67890"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "things": [
            {
              "id": 12345,
              "name": "Thing 1"
            },
            {
              "id": 67890,
              "name": "Thing 2"
            }
          ]
        },
        "matchingRules": {
          "$.body.things[0].name": {
            "match": "type"
          },
          "$.body.things[1].name": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}